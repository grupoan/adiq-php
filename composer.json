{
    "name": "grupoan/adiq-php",
    "description": "Adiq PHP Library",
    "type": "library",
    "keywords": [
      "adiq",
      "gateway",
      "api"
    ],
    "license": "MIT",
    "require": {
      "php": ">=5.6.0",
      "guzzlehttp/guzzle": "^7.4.5"
    },
    "require-dev": {
      "phpunit/phpunit": "^8",
      "squizlabs/php_codesniffer": "^3.3"
    },
    "scripts": {
      "test": [
        "@composer install",
        "vendor/bin/phpunit"
      ]
    },
    "autoload": {
      "psr-4": {"Adiq\\": "src/"}
    },
    "autoload-dev": {
      "psr-4": {
        "Adiq\\Test\\": "tests/unit"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.1.2"
  }
